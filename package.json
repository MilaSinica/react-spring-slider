{
  "name": "@farbenmeer/react-spring-slider",
  "version": "0.0.1-alpha.1",
  "main": "dist/index.js",
  "author": {
    "name": "Max Str√ºbing",
    "email": "max.struebing@farbenmeer.de",
    "url": "https://maex.me"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farbenmeer/react-spring-slider.git"
  },
  "bugs": {
    "url": "https://github.com/farbenmeer/react-spring-slider/issues"
  },
  "homepage": "https://github.com/farbenmeer/react-spring-slider",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,css}": [
      "npm run lint:fix",
      "npm run lint",
      "npm run lint:editorconfig",
      "git add"
    ]
  },
  "scripts": {
    "build": "babel src/index.js --out-file dist/index.js && ln -sf ../src/index.css dist/index.css",
    "watch": "babel --watch src --out-file dist/index.js",
    "lint": "prettier-eslint $PWD/src/**/*",
    "lint:editorconfig": "ec",
    "lint:fix": "prettier-eslint --write $PWD/src/**/*",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "lodash": "4.17.15",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "dependencies": {
    "react-spring": "8.0.27",
    "react-use-gesture": "6.0.14"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "babel-loader": "^8.0.6",
    "commitlint": "^8.2.0",
    "editorconfig-checker": "^3.0.3",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "lodash": "4.17.15",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "prettier-eslint-cli": "^5.0.0",
    "prop-types": "15.7.2"
  }
}
